  <script nonce="">((function() {
    function initDcoPixel(trackingStateDco) {
        function hasSatellite() {
            return typeof _satellite !== "undefined"
        }

        function buildPixel(params) {

            var protocol = (location.protocol == "https:" ? "https:" : "http:");
            var url = "//dp.tinkoff.ru/pixel";

            var parameters = "";
            var isFirstParam = true;

            for (var key in params) {
                if (params.hasOwnProperty(key)) {
                    if (isFirstParam) {
                        parameters += "?";
                        isFirstParam = false;
                    }
                    else {
                        parameters += "&";
                    }

                    parameters += key + "=" + params[key];
                }
            }

            return protocol + url + parameters;
        }

        function addPixel(pixel) {

            var image = document.createElement('img');
            image.src = pixel;
        }

        function trackTimer(pixel, timer) {

            setTimeout(function(){ addPixel(pixel); }, timer);
        }

        function getQueryParam(name) {

            var query = window.location.search.substring(1);
            var params = query.split("&");

            for (var i=0;i<params.length;i++) {
                var param = params[i].split("=");
                if (param[0] == name) {
                    return param[1];
                }
            }

            return "";
        }

        function getIDCode() {

            var idcode = 25;
            

            if (hasSatellite()) {

                var product = _satellite.getVar('Product Name');
                var relativeURL = _satellite.getVar('Relative URL');
                var iFrameURL = _satellite.getVar('Iframe URL');
                var form = _satellite.getVar('Form');
                var absolutURL = _satellite.getVar('Absolut URL');

                if (product && product == 'Tinkoff Platinum') {
