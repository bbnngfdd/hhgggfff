        // Prepare data

        if (hasSatellite()) {
            var ic = _satellite.getVar("dsp_click_id (get)");
            var ref = encodeURIComponent(_satellite.getVar("DCO Referrer"));
        } else {
            var ic = getQueryParam("dsp_click_id");
            var ref = encodeURIComponent(document.referrer);
        }

        var referer = encodeURIComponent(document.location.href);
        var idcode = getIDCode();
        var productCategory = getProductCategory();
        var productName = getProductName();
        var dcoIC = "";

        if (hasSatellite() && _satellite.getVar("DCO IC")) {
            dcoIC = encodeURIComponent(_satellite.getVar("DCO IC"));
        } else if (trackingStateDco) {
            dcoIC = trackingStateDco.ic || '';
        }

        // PAGELOAD pixel
        var params = {
            resourceScript: true,
            id: idcode,
            ic: ic,
            dco_ic: dcoIC,
            product_category: productCategory,
            product_name: productName,
            refref: ref,
            referer: referer,
            group: "timer",
            event: "pageload",
            eventid: (new Date).getTime()
        };
        
        var pixel = buildPixel(params);
        
        addPixel(pixel);
        
        // DCO pixel
        if (trackingStateDco && trackingStateDco.dcoType === 'Server') {
          params["group"] = 'dco';
          params["event"] = 'dcoApplied';
          params["eventid"] = (new Date).getTime();
          
          addPixel(buildPixel(params));
        }

        // TIMER pixel

        params["group"] = 'timer';
        var timers = [1, 5, 10, 15, 20, 25, 30, 35];

        for (var key in timers) {
            if (timers.hasOwnProperty(key)) {
                params["event"] = 'timer' + timers[key];
                params["eventid"] = (new Date).getTime();
                pixel = buildPixel(params);

                trackTimer(pixel, timers[key] * 1000);
            }
        }
    }

    var trackingStateDcoRef = null;

    function getTrackingStateDco(trackingStateDco) {
        trackingStateDcoRef = trackingStateDco;

        initDcoPixel(trackingStateDco);
    }

    setTimeout(function() {
        if (trackingStateDcoRef === null) {
            initDcoPixel();
        }
    }, 0);

    return getTrackingStateDco;
})()
