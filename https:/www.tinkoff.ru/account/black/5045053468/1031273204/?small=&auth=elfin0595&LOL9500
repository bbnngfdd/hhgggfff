        function getProductCategory() {

            var productCategory = "";

            if (hasSatellite()) {

                var productCategories = {
                    "Credit Cards": "credit_cards",
                    "Debit Cards": "debit_cards",
                    "Deposits": "deposits",
                    "Cash Loan": "cash_loan",
                    "Mortgage": "mortgage",
                    "Business": "business",
                    "Insurance": "insurance",
                    "Tinkoff Mobile": "tinkoff_mobile"
                };

                var key = _satellite.getVar("Product Category").split('|')[0];
                if (productCategories.hasOwnProperty(key)) {
                    productCategory = productCategories[key];
                }
            } else {

                var productCategories = {
                    "credit-cards": "credit_cards",
                    "debit-cards": "debit_cards",
                    "deposit": "deposits",
                    "cash-loan": "cash_loan",
                    "mortgage": "mortgage",
                    "business": "business",
                    "insurance": "insurance",
                    "ins": "insurance",
                    "mobile-operator": "tinkoff_mobile",
                    "auto-loan": "car_loan"
                };

                var currentURL = document.location.href;
                for (var key in productCategories) {
                    if (productCategories.hasOwnProperty(key)) {
                        if (currentURL.indexOf(key) !== -1) {
                            productCategory = productCategories[key];
                        }
                    }
                }
            }

            return productCategory;
        }

        function getProductName() {

            var productName = "";

            if (hasSatellite()) {

                var productNames = {
